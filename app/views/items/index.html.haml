.ii-wrapper
  .ii-top
    .ii-menu
      .ii-info 画像が挿入されます
      .ii-tag
        %h1.ii-tag--title 人気のカテゴリー
        .ii-tag__categorys
          %ul.ii-tag__categorys__list
            %li.ii-tag__categorys__list__name レデイース
            %li.ii-tag__categorys__list__name メンズ
            %li.ii-tag__categorys__list__name ベビー
      .ii-contents
        .ii-contents-nav
          .ii-contents-nav__name
            %h1.ii-contents-nav__name-title 商品一覧
            = link_to "もっとみる >"
          .ii-contents-nav__items
            .ii-contents-nav__items__list
              %ul.ii-contents-nav__items__list_items
                %li.ii-contents-nav__items__list__items__item
                  .ii-contents-nav__items__list__items__item__link
                    = link_to "", class: "ii-link" do
                      %figure.ii-link__btn
                        %figcaption
                          %span
                        .ii-link__btn__img
                          %span
  .ii-camera
    = link_to "", class: "ii-camera__btn" do
      .ii-camera__btn__box
        .ii-camera__btn__box--text 出品
        = icon('fas', 'camera')